{
  "apps": [
    {
      "name": "adafruit",
      "script": "adafruit.js",
      "log_date_format": "YYYY-MM-DD HH:mm",
      "ignoreWatch": [
        "node_modules"
      ],
      "watch": false,
      "args": [
        "--color",
        "--no-warnings"
      ],
      "env": {
        "DEBUG_COLORS": true
      }
    }
  ]
}